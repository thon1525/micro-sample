{
  "name": "users-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --legacy-watch -r tsconfig-paths/register src/server.ts",
    "start": "pm2 start ./build/ecosystem.config.js",
    "stop": "pm2 stop all",
    "restart": "pm2 restart ./build/ecosystem.config.js",
    "delete": "pm2 delete all",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && cp -R configs ./build/ && cp public_key.pem ./build/ && cp ecosystem.config.js ./build/ && cp package.json ./build/",
    "test": "cross-env NODE_ENV=testing jest --coverage=true --detectOpenHandles --watchAll=false",
    "gen:spec": "tsoa spec",
    "gen:routes": "tsoa routes",
    "gen:all": "npm run gen:spec && npm run gen:routes"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "on-headers": "^1.0.2",
    "redoc-express": "^2.1.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7",
    "winston": "^3.13.0",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/amqplib": "^0.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/on-headers": "^1.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-alias": "^0.0.7",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}